#Add a item
a=(4,6,2,8,3,1)
print(a)
a=a+(9,)
print(a)
a=a[:5]+(15,20,25)+a[:5]
print(a)
list=list(a)
list.append(30)
a=tuple(list)
print(a) 


#Length of a tuple
n=input()
c=tuple(n)
d=len(n)
print(c)
print(d)


#Reverse a tuple
n=input()
n=tuple(n)
print(n[::-1])


#Repeated element in a tuple
a=(2,4,5,6,2,3,4,4,7)
print(a)
temp=a.count(5)
print(temp)
